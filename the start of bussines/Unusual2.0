body{
  background-image:
  url(alone.jpg);
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  background-attachment: fixed;
}
h1{
  color:azure;
}
font{
  color:azure;
  font-size: x-large;
  font-style: italic;
}
pre{
  color:azure;
  font-size: larger;
}
.purple-button{
  background-color:rgb(204, 162, 204);
  border-style: dotted;
  border-radius: 50px;
  width: 10%;
}
.black-title{
  color:black;
}
.dark-title{
  color:black;
}
.green-button{
  background-color:aqua;
  width:200px;
  height:230px;
}
.orange-button{
  background-color: orangered;
  border-radius: 50%;
  height: 60px;
  width: 60px;
  border: none;
}
.yellow-button{
  background-color: yellow;
  border-radius: 50%;
  height: 60px;
  width: 60px;
  border: none;
}
.cal{
  background-color: orange;
  border-radius: 100%;
  height: 50px;
  width: 50px;
  border: none;
}
.game-button{
  border-style: dotted;
  border-radius: 100%;
  height: 70px;
  width: 70px;
  background-color: green;

}
